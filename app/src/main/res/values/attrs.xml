<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CheckmarkView">
    <attr name="imageSize" format="dimension"/>
  </declare-styleable>

  <declare-styleable name="GradientPalette">
    <attr name="android:orientation">
      <enum name="horizontal" value="0"/>
      <enum name="vertical" value="1"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="Image">
    <attr name="imageSrc" format="reference"/>
  </declare-styleable>

  <declare-styleable name="VerticalSeekbar">
    <attr name="cornersRadius" format="dimension"/>
    <attr name="lineVerticalOffset" format="dimension"/>
    <attr name="lineWidth" format="dimension"/>
  </declare-styleable>
</resources>